<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TCTermButton">
        <attr name="description" format="string" />
        <attr name="term" format="string" />
        <attr name="type" format="enum">
        <enum name="NON_MINUS_OPERATOR" value="0"/>
        <enum name="DECIMAL" value="1"/>
        <enum name="INTEGER" value="2"/>
        <enum name="MINUS_OPERATOR" value="3"/>
        <enum name="LEFT_BRACKET" value="4"/>
        <enum name="RIGHT_BRACKET" value="5"/>
   		</attr>
    </declare-styleable> 
</resources>
